{
  "name": "meet-bot",
  "version": "1.0.0",
  "description": "Google Meet Bot with Docker and Browserless Chrome",
  "main": "docker-server.js",
  "scripts": {
    "start": "node docker-server.js",
    "start:dev": "NODE_ENV=development node docker-server.js",
    "start:prod": "NODE_ENV=production node docker-server.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart",
    "clean": "rm -rf logs/*.log"
  },
  "keywords": [
    "google-meet",
    "bot",
    "puppeteer",
    "docker",
    "browserless"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mui/material": "^7.0.2",
    "@react-oauth/google": "^0.12.1",
    "axios": "^1.8.4",
    "body-parser": "^1.20.3",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "firebase": "^11.6.0",
    "lucide-react": "^0.503.0",
    "puppeteer": "^22.15.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.0",
    "typescript": "^5.8.3"
  }
}
